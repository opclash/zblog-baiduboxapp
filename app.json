{
    "pages": [
        "pages/home/index",
        "pages/fenxiang/index",
        "pages/luyou/index",
        "pages/daima/index",
        "pages/say/index",
        "pages/article/index",
        "pages/page/index",
        "pages/search/index",
        "swan-sitemap/index",
        "chaxun/beian/index"
    ],
    "window": {
        "backgroundTextStyle": "dark",
        "navigationBarBackgroundColor": "#338ff0",
        "navigationBarTextStyle": "white"
    },
    "dynamicLib": {
        "myDynamicLib": {
            "provider": "swan-interaction"
        },
        "swan-sitemap-lib": {
            "provider": "swan-sitemap"
        },
        "swanGov": {
            "provider": "swan-gov"
        },
        "smart-sc": {
            "provider": "smart-sc"
        }
    },
    "tabBar": {
        "color": "#80848f",
        "selectedColor": "#338ff0",
        "backgroundColor": "#f7f7fa",
        "list": [
            {
                "pagePath": "pages/home/index",
                "iconPath": "images/icon/Planet.png",
                "selectedIconPath": "images/icon/Planet_fill.png",
                "text": "Home"
            },
            {
                "pagePath": "pages/luyou/index",
                "iconPath": "images/icon/luyou.png",
                "selectedIconPath": "images/icon/luyou_fill.png",
                "text": "route"
            },
            {
                "pagePath": "pages/fenxiang/index",
                "iconPath": "images/icon/fenxiang.png",
                "selectedIconPath": "images/icon/fenxiang_fill.png",
                "text": "share"
            },
            {
                "pagePath": "pages/say/index",
                "iconPath": "images/icon/say.png",
                "selectedIconPath": "images/icon/say_fill.png",
                "text": "talklee"
            },
            {
                "pagePath": "pages/daima/index",
                "iconPath": "images/icon/daima.png",
                "selectedIconPath": "images/icon/daima_fill.png",
                "text": "share"
            }
        ]
    },
    "requiredBackgroundModes": ["audio"],
    "networkTimeout": {
        "request": 30000,
        "connectSocket": 10000,
        "uploadFile": 10000,
        "downloadFile": 10000
    },
    "permission": {
        "scope.userLocation": {
            "desc": "你的位置信息将用于小程序位置接口的效果展示"
        }
    },
    "prefetch" : {
        "enabled": true
    },
    "cookie": {
        "enableStore": true
    },
    "useSwanNews": true,
    "debug": true
}